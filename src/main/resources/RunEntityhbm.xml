<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="it.costanza.model.databaseBeans2.RunEntity" table="RUN" schema="EUROPE">
        <id name="idRun">
            <column name="ID_RUN" sql-type="bigint(100)"/>
        </id>
        <property name="dataInizioRun">
            <column name="DATA_INIZIO_RUN" sql-type="datetime" not-null="true"/>
        </property>
        <property name="dataFineRun">
            <column name="DATA_FINE_RUN" sql-type="datetime" not-null="true"/>
        </property>
        <property name="tipoRun">
            <column name="TIPO_RUN" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="it.costanza.model.databaseBeans2.TurniGeneratiEntity" table="TURNI_GENERATI" schema="EUROPE">
        <id name="idTurno">
            <column name="ID_TURNO" sql-type="bigint(15)"/>
        </id>
        <property name="dataTurno">
            <column name="DATA_TURNO" sql-type="date" not-null="true"/>
        </property>
        <property name="tipoTurno">
            <column name="TIPO_TURNO" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="ruoloTurno">
            <column name="RUOLO_TURNO" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="personaTurno">
            <column name="PERSONA_TURNO" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="idRun">
            <column name="ID_RUN" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
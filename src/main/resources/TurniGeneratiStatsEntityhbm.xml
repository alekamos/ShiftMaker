<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="it.costanza.model.databaseBeans2.TurniGeneratiStatsEntity" table="TURNI_GENERATI_STATS"
           schema="EUROPE">
        <property name="idTurno">
            <column name="ID_TURNO" sql-type="bigint(15)" not-null="true"/>
        </property>
        <property name="mediaTurniTot">
            <column name="MEDIA_TURNI_TOT" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdevTurniTot">
            <column name="SDEV_TURNI_TOT" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="mediaPresFest">
            <column name="MEDIA_PRES_FEST" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdevPresFest">
            <column name="SDEV_PRES_FEST" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="mediaGiorniFest">
            <column name="MEDIA_GIORNI_FEST" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdevGiorniFest">
            <column name="SDEV_GIORNI_FEST" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="mediaGiorniFer">
            <column name="MEDIA_GIORNI_FER" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdevGiorniFer">
            <column name="SDEV_GIORNI_FER" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="mediaNotti">
            <column name="MEDIA_NOTTI" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdevNotti">
            <column name="SDEV_NOTTI" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdev1Settimana">
            <column name="SDEV_1_SETTIMANA" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdev2Settimana">
            <column name="SDEV_2_SETTIMANA" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdev3Settimana">
            <column name="SDEV_3_SETTIMANA" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdev4Settimana">
            <column name="SDEV_4_SETTIMANA" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="sdev5Settimana">
            <column name="SDEV_5_SETTIMANA" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="score">
            <column name="SCORE" sql-type="double(5,5)" precision="5" scale="5" not-null="true"/>
        </property>
        <property name="scoreFormula">
            <column name="SCORE_FORMULA" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="dataInserimento">
            <column name="DATA_INSERIMENTO" sql-type="datetime" not-null="true"/>
        </property>
        <property name="dataAggiornamento">
            <column name="DATA_AGGIORNAMENTO" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>